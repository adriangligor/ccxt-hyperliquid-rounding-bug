{
    "name": "adriangligor/ccxt-playground",
    "version": "1.0.0",
    "description": "Playground for CCXT library",
    "type": "project",
    "authors": [
        {
            "name": "Adrian Gligor"
        }
    ],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zend-opcache": "*",
        "ext-zip": "*",
        "ccxt/ccxt": "4.4.86"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
